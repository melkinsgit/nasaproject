// other Favorites jade
extends layout

block content

	h1 OTHER FAVORITES

	if favorites.length != 0
		each fav in favorites

			.favitem
				a(href="#{fav.apodurl}")
					img.favimage(src="#{fav.url}")
				p.favtitle #{fav.title}
				p.favdate #{fav.date}

	else
		p No favorites saved yet.

	include buttons